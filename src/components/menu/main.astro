---
const links = [
    { src: "私について", link: "profile" },
    { src: "このサイトについて", link: "about" },
    { src: "unnamedworks", link: "unnamedworks" },
];
---

<style>
    div {
        width: auto;
        text-align: center;
        display: inline-block;
        margin: 0;
    }
    ul {
        padding: 0px;
        margin: 0px;
    }
    ul li:before {
        content: "";
        position: absolute;
        top: 0px;
        left: 0px;
    }

    ul li {
        list-style-type: none;
        position: relative;
        padding-left: 0px;
        line-height: 1.3;
        font-size: 25px;
    }
</style>
<div>
    <ul>
        {
            links.map((item) => (
                <li>
                    <a href={item.link}>{item.src}</a>
                </li>
            ))
        }
    </ul>
</div>
