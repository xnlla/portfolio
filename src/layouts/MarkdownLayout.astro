---

import BaseLayout from './BlogLayout.astro';
const { frontmatter } = Astro.props;
const pageTimestamp = frontmatter.pubDate.slice(0,10);
const pageUrl = "/post/" + String(pageTimestamp)  
---
<BaseLayout 
    pageTitle={frontmatter.title},
    pageTags={frontmatter.tags},
    pageTimestamp={pageTimestamp},
    pageUrl={pageUrl}
    >
<slot />
</BaseLayout>